
#|
;switch case

(define-syntax դիտարկել
	(syntax-rules (դեպք անել)
		((դիտարկել արտահայտություն (դեպք ա անել գործ1) (դեպք բ անել գործ2) ...)
		(begin (if ա գործ1)
		(if բ գործ2)
		...)
		)
	)
)

|#

#|
(define a 25)
(դիտարկել a
	 (դեպք (< a 10)
	  անել (display 11111 ))
	 (դեպք (> a 15)
	  անել (display "այոոոո"))
) 
#|



